# Binance API credentials for live trading
# Replace with your actual API key and secret for live trading
BINANCE_API_KEY=your_live_api_key_here
BINANCE_API_SECRET=your_live_api_secret_here

# Binance API credentials for testnet trading
# Replace with your actual API key and secret for testnet trading
BINANCE_TESTNET_API_KEY=your_testnet_api_key_here
BINANCE_TESTNET_API_SECRET=your_testnet_api_secret_here

# Bot execution mode: 'trade' for live trading, 'test' for testnet
BOT_MODE=test

# Default lookback period in days for backtesting
BACKTEST_DEFAULT_LOOKBACK_DAYS=10
